    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["-p", "PlanningAgent", "-l", "knownMedium"] //"${command:pickArgs}"
        },
        {
            "name": "Python Debugger: Pacman",
            "type": "debugpy",
            "request": "launch",
            "program": "pacman.py",
            "console": "integratedTerminal",
            "args": ["-p", "PlanningAgent", "-l", "knownMedium"]
        }
    ]
}